        max-width: 100%;
        height: 388px;
        margin: 0 auto;
    }

    .service_bottom {
        width: 95%;
    }

    .post-comment .pull-left img {
        margin-right: 20px;
        /*border-left: 3px solid #0884d5;*/
    }
</style>
<section id="banner" style="display:none;">
    <!-- Slider -->
    <div id="main-slider" class="flexslider">
        <ul class="slides" id="oldlunbo">
            @{
                if (Model.Count > 0)
                {
                    var cnt = 0;
                    foreach (var item in Model)
                    {
                        if (cnt >= 5)
                        {
                            break;
                        }
                        <li>
                            <img src="@item.PicUrl" alt="" />
                            <div class="flex-caption">
                                <h3>@item.PicTitle</h3>
                                <p>@item.Memo</p>
                            </div>
                        </li>
                        cnt++;
                    }
                }
            }
        </ul>
    </div>
    <!-- end slider -->
</section>

