
@{
    ViewBag.Title = "服务人员详情";
    Layout = null;
}
@model CareCenter2.DB.tb_PartTime
<link href="/Content/layui/css/layui.css" rel="stylesheet" />
<style>
    .layui-form-label {
        width: 100px;
    }

    .layui-input, .layui-textarea {
        display: block;
        width: 60%;
        padding-left: 10px;
    }
</style>
<div class="main-page signup-page">
    <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
        <legend>人员信息</legend>
    </fieldset>
    <form class="layui-form" method="post" enctype="multipart/form-data">
        <div class="layui-form-item">
            <label class="layui-form-label">名称</label>
            <div class="layui-input-block">
                <input type="text" name="AdminID" id="AdminID" lay-verify="required" autocomplete="off" placeholder="请输入管理员ID" value="@Model.UserName" class="layui-input" disabled>
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">手机号码</label>
            <div class="layui-input-block">
                <input type="text" name="NickName" id="NickName" lay-verify="required" autocomplete="off" value="@Model.Phone" class="layui-input"disabled>
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">性别</label>
            <div class="layui-input-block">
                <input type="text" name="AdminPwd" id="AdminPwd" lay-verify="required" autocomplete="off"value="@Model.Sex " class="layui-input" disabled>
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">申请职位</label>
            <div class="layui-input-block">
                <input type="text" name="RealName" id="RealName" lay-verify="required" autocomplete="off" value="@Model.Post" class="layui-input"disabled>
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">工作年限</label>
            <div class="layui-input-block">
                <input type="text" name="Mobile" id="Mobile" lay-verify="required|phone" autocomplete="off" value="@Model.YearWork" class="layui-input"disabled>
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">兼职时间</label>
            <div class="layui-input-block">
                <input type="text" name="Mobile" id="Mobile" lay-verify="required|phone" autocomplete="off" value="@Model.DateRange" class="layui-input"disabled>
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">学历</label>
            <div class="layui-input-block">
                <input type="text" name="Mobile" id="Mobile" lay-verify="required|phone" autocomplete="off" value="@Model.Education" class="layui-input"disabled>
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">薪资</label>
            <div class="layui-input-block">
                <input type="text" name="Mobile" id="Mobile" lay-verify="required|phone" autocomplete="off" value="@Model.Salary" class="layui-input"disabled>
            </div>
        </div>     

        <div class="layui-form-item">
            <div class="layui-input-block ">
                @*<button class="layui-btn" lay-submit="" lay-filter="UploadFile">注册</button>*@
                @*<button type="reset" class="layui-btn layui-btn-primary">重置</button>*@
            </div>
        </div>
    </form>
</div>





